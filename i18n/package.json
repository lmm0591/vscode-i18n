{
    "name": "vscode-i18n",
    "displayName": "vscode-i18n",
    "description": "",
    "version": "0.0.1",
    "publisher": "lmm0591",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:plaintext"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }]
    },
    "configuration": {
        "type": "object",
        "title": "vscode-i18n",
        "properties": {
            "i18n.dir": {
                "type": "array",
                "items": {
                    "anyOf": [{"type": "string"}]
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "vscode-languageclient": "^2.6.3"
    }
}